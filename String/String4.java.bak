class String4
{
	public static void main(String[] args) 
	{
		String s1="prolog";
		String s2="prolog";
		String s3=new String("prolog");
		String s4=new String("prolog");
		String s5=s3.intern();  //method that return the reference of String constant poolat the time of object creation in heap.
		if(s5==s2)
			System.out.println("same object");
		if(s3!=s4)
		System.out.println("different object");
	}
}
